#!/bin/bash
DIR=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )
source $DIR/environment

$DIR/check
if [ $? -eq 0 ]; then
    docker-compose run -w /polyaxon/api --rm --name=polyaxon_worker web celery -A api worker --without-mingle --without-gossip --loglevel=DEBUG -Q api.experiments,api.clusters,events.namespace,events.resources,logs.sidecars,stream.logs.sidecars --concurrency=4 -n default@%h
fi
