{
  "name": "polyaxon-ui",
  "version": "0.1.1",
  "description": "Polyaxon ui",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/* && rm -rf public/dist/* && mkdir public/dist/js && mkdir public/dist/css && mkdir public/dist/fonts",
    "_copy_react": "cp node_modules/react/umd/react.production.min.js node_modules/react-dom/umd/react-dom.production.min.js public/dist/js",
    "_copy_css": "cp node_modules/font-awesome/css/font-awesome.min.css node_modules/bootstrap/dist/css/bootstrap.min.css public/dist/css",
    "_copy_fonts": "cp -r node_modules/font-awesome/fonts public/dist/",
    "copy": "npm run _copy_react && npm run _copy_css && npm run _copy_fonts",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output public/dist/js/bundle.min.js",
    "dev": "webpack-dev-server --host 0.0.0.0 --port 8001 --env.dev --config webpack.config.dev.js  --output-public-path=dist/",
    "build": "npm run clean && webpack --env.prod --config webpack.config.prod.js && npm run minify && npm run copy"
  },
  "author": "mourad mourafiq",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "8.10.20",
    "@types/redux-logger": "3.0.6",
    "awesome-typescript-loader": "3.5.0",
    "bootstrap": "3.3.7",
    "css-loader": "0.28.11",
    "file-loader": "0.11.2",
    "font-awesome": "4.7.0",
    "fontawesome": "4.7.2",
    "less": "2.7.3",
    "less-loader": "4.1.0",
    "redux-logger": "3.0.6",
    "source-map-loader": "0.2.3",
    "style-loader": "0.18.2",
    "tslint": "5.10.0",
    "tslint-react": "3.6.0",
    "typescript": "2.9.2",
    "uglify-js": "3.4.2",
    "url-loader": "0.5.9",
    "webpack": "3.12.0",
    "webpack-dev-server": "2.11.2"
  },
  "dependencies": {
    "@types/deep-equal": "1.0.1",
    "@types/history": "4.6.2",
    "@types/js-cookie": "2.1.0",
    "@types/lodash": "4.14.110",
    "@types/query-string": "6.1.0",
    "@types/raven": "2.5.1",
    "@types/react": "16.4.2",
    "@types/react-bootstrap": "0.32.10",
    "@types/react-data-grid": "2.0.11",
    "@types/react-dom": "16.0.6",
    "@types/react-redux": "6.0.2",
    "@types/react-router": "4.0.27",
    "@types/react-router-bootstrap": "0.24.5",
    "@types/react-router-dom": "4.2.7",
    "@types/redux-form": "7.3.0",
    "@types/sprintf-js": "0.0.27",
    "deep-equal": "1.0.1",
    "history": "4.7.2",
